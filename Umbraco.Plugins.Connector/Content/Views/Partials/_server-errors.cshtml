@inherits Umbraco.Web.Mvc.UmbracoViewPage
@using Umbraco.Plugins.Connector.Models
@foreach (var code in Enum.GetValues(typeof(ApiPayloadErrorCodes)))
{
    var name = Enum.GetName(typeof(ApiPayloadErrorCodes), code);
    var dictionaryValue = Umbraco.GetDictionaryValue($"[ServerErrors]{name}", name);
<input type="hidden" id="error-@Convert.ToInt32(code)" value="@dictionaryValue" />
}
