@section Scripts{
    <script type="text/javascript" src="~/scripts/owl.carousel.min.js"></script>
    <script>
        $(document).ready(function () {
            $('.slider-big').owlCarousel({
                loop: false,
                margin: 8,
                nav: true,
                autoWidth: true,
                responsive: {
                    0: {
                        items: 1,
                        autoWidth: false
                    },
                    541: {
                        items: 3
                    }
                }
            });
            $('.slider-small').owlCarousel({
                loop: false,
                margin: 8,
                nav: true,
                autoWidth: true,
                items: 4,
                responsive: {
                    0: {
                        items: 1
                    },
                    721: {
                        items: 3
                    },
                    1000: {
                        items: 4
                    }
                }
            });
            $('.slider-affiliates').owlCarousel({
                items: 1,
                loop: true,
                margin: 8,
                nav: true,
                dot: true,
                // autoplay: true,
                autoplayTimeout: 4700,
                smartSpeed: 900
            });
            $('.slider-testimonials').owlCarousel({
                items: 2,
                loop: true,
                margin: 16,
                nav: true,
                dot: true,
                autoWidth: true,
                // autoplay: true,
                autoplayTimeout: 4700,
                smartSpeed: 900,
                responsive: {
                    0: {
                        items: 3,
                        autoWidth: true,
                        margin: 8,
                        center: true
                    },
                    541: {
                        items: 3,
                        autoWidth: true,
                        margin: 8,
                        center: true
                    }
                },
                onInitialized: fixOwlCarousel,
                onRefreshed: fixOwlCarousel
            });
            $(".accordeon-faq dd").hide().prev().click(function () {
                $(this).parents(".accordeon-faq").find("dd").not(this).slideUp().prev().removeClass("active");
                $(this).next().not(":visible").slideDown().prev().addClass("active");
            });
        });
        var fixOwlCarousel = function () {
            stage = $('.owl-stage');
            stage.width(stage.width() * 2);
        }
    </script>
}
