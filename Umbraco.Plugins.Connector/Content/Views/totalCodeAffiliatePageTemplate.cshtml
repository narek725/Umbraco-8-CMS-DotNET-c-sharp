<section class="section-slider">
    <div class="top-slider  @(HttpContext.Current.Request.Url.ToString().Contains("/en/")? "sliderbg":"")">
        <div class="slider-affiliates owl-carousel">
            @foreach (var item in Model.Slider)
            {
                <div class="item">
                    <div class="affiliates-item">
                        <div class="affiliates-item--text">
                            <span>@item.Title</span>
                            <p>@item.Subtitle</p><a class="button send-button" href="@Model.JoinNowLink">@Umbraco.GetDictionaryValue("[Common]JoinNow")</a>
                        </div>
                        <div class="affiliates-item--img"><img src="@item.Image" alt="" style="right: -167px; top: 35px;"></div>
                    </div>
                </div>
            }
        </div>
    </div>
</section>
<section class="section-partners">
    <div class="wrapper-partners">
        <div class="container-center">
            <div class="default-names">
                <h3>@Model.partners.Heading</h3>
                <p>@Model.partners.SubHeading</p>
            </div>
            <div class="slider-partners owl-carousel">
                @foreach (var partner in Model.partners.PartnerList)
                {
                    var Image = partner.HasValue("mainImage") ? partner.GetProperty("mainImage").Value<IPublishedContent>().Url : string.Empty;
                    var Title = partner.HasValue("title") ? partner.GetProperty("title").GetValue().ToString() : string.Empty;
                    var Subtitle = partner.HasValue("subTitle") ? partner.GetProperty("subTitle").GetValue().ToString() : string.Empty;

                    var partnerNumbers = partner.Value<IEnumerable<IPublishedElement>>("partnersNumbers")?.ToList();
                    if (partnerNumbers.Count > 0)
                    {
                        <div class="item big-item">
                            <div class="partners-item">
                                <div class="partners-tops">
                                    <div class="partners-icon"><img src="@Image" alt=""></div>
                                    <div class="partners-text">
                                        <h4>@Title</h4>
                                        <p>@Subtitle</p>
                                    </div>
                                </div>
                                <div class="partners-number">
                                    @{
                                        var i = 0;
                                        foreach (var item in partnerNumbers)
                                        {
                                            if (i == 0)
                                            {
                                                <div class="partners-number--items" style="margin-right: 75px;">
                                                    <h4>@item.Value("number")</h4>
                                                    <p>@item.Value("itemName")</p>
                                                </div>
                                            }
                                            else if (i == 1)
                                            {
                                                <div class="partners-number--items" style="margin-right: 90px;">
                                                    <h4>@item.Value("number")</h4>
                                                    <p>@item.Value("itemName")</p>
                                                </div>
                                            }
                                            else
                                            {
                                                <div class="partners-number--items">
                                                    <h4>@item.Value("number")</h4>
                                                    <p>@item.Value("itemName")</p>
                                                </div>
                                            }



                                            i++;
                                        }
                                    }
                                </div>
                            </div>
                        </div>
                    }
                    else
                    {
                        <div class="item">
                            <div class="partners-item">
                                <div class="partners-icon"><img src="@Image" alt=""></div>
                                <div class="partners-text">
                                    <h4>@Title</h4>
                                    <p>@Subtitle</p>
                                </div>
                            </div>
                        </div>
                    }
                }





            </div>
            <div class="wrapper-button text-center"><a class="button send-button" href="@Model.JoinNowLink">@Umbraco.GetDictionaryValue("[Common]JoinNow")</a></div>
        </div>
    </div>
</section>

