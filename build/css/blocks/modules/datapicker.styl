.ui-datepicker
	position relative
	width 240px
	fsR(14px)
	c cWhite
	background: #2f2d2f
	brr(6px)
	box-shadow: 0 5px 30px rgba(5,5,5,.35)
	&:before
		position absolute
		top 0
		left 0
		width 100%
		height 100%
		display block
		content ""
		box-shadow 0 0 0 1px #403f40 inset
		z-index -1
		brr(6px)
	
.ui-datepicker-header
	display flex
	flex-direction row
	justify-content space-between
	padding 10px 5px
	.ui-datepicker-prev,
	.ui-datepicker-next
		cursor pointer
	.ui-datepicker-prev
		order 1
	.ui-datepicker-next
		order 3
	.ui-datepicker-title
		order 2

.ui-datepicker-unselectable .ui-state-default
	c cGray !important

.ui-datepicker-calendar .ui-state-default
	c cWhite
	text-decoration none
	line-height 28px

.ui-datepicker td
	text-align center

.ui-datepicker-calendar
	width 100%
	border-collapse collapse
	thead
		border-top 1px solid #403f40
		border-bottom 1px solid #403f40
		tr
			th
				padding 5px 0 5px

.ui-datepicker th
	fsSB(14px)
	c #6f6e70